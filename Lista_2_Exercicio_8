#include<stdio.h>
#include<locale.h>
#include<string.h>

int main(){
	
	int i, tab0, tab1, tab2, tab3, tab4, tab5, tab6, tab7, tab8, tab9;
	
	//Tabuada de 0
	for(i=0; i<=10; i++){
		
		//Tabuada de 0
		tab0 = 0*i;
		printf("Tabuada de 0: 0 x %i = %i\n", i, tab0);

	}
			printf("\n");
	//Tabuada de 1
	for(i=0; i<=10; i++){

		tab1 = 1*i;
		printf("Tabuada de 1: 1 x %i = %i\n", i, tab1);	
	}
			printf("\n");
	//Tabuada de 2
	for(i=0; i<=10; i++){

		tab2 = 2*i;
		printf("Tabuada de 2: 2 x %i = %i\n", i, tab2);
	}
		printf("\n");
	//Tabuada de 3
	for(i=0; i<=10; i++){

		tab3 = 3*i;
		printf("Tabuada de 3: 3 x %i = %i\n", i, tab3);
	}
		printf("\n");
	//Tabuada de 4
	for(i=0; i<=10; i++){

		//Tabuada de 4
		tab4 = 4*i;
		printf("Tabuada de 4: 4 x %i = %i\n", i, tab4);
	}
			printf("\n");
	//Tabuada de 5
	for(i=0; i<=10; i++){

		//Tabuada de 5
		tab5 = 5*i;
		printf("Tabuada de 5: 5 x %i = %i\n", i, tab5);
	}
		printf("\n");	
	//Tabuada de 6
	for(i=0; i<=10; i++){

		//Tabuada de 6
		tab6 = 6*i;
		printf("Tabuada de 6: 6 x %i = %i\n", i, tab6);
	}
		printf("\n");	
	//Tabuada de 7
	for(i=0; i<=10; i++){

		//Tabuada de 7
		tab7 = 7*i;
		printf("Tabuada de 7: 7 x %i = %i\n", i, tab7);
	}
		printf("\n");	
	//Tabuada de 8
	for(i=0; i<=10; i++){

		//Tabuada de 8
		tab8 = 8*i;
		printf("Tabuada de 8: 8 x %i = %i\n", i, tab8);
	}
		printf("\n");	
	//Tabuada de 9
	for(i=0; i<=10; i++){

		//Tabuada de 9
		tab9 = 9*i;
		printf("Tabuada de 9: 9 x %i = %i\n", i, tab9);
	}
	

	
	
	
}
