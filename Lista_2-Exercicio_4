#include<stdio.h>
#include<locale.h>
#include<string.h>

int main(){
	//Variaveis
	int i, j = 0;
	
	//Extrutura de Repetição
	for(i=0; i<=99; i++){
		
		printf("%i", i);
		
		//Contador
		j++;	
			//Condição para quando o contador chegar a 10 pular uma linha e zera!
			if(j == 10){
				printf("\n");
				j = 0;
			}
		
	}
	
	
}
